<Window x:Class="DatabaseManagementSystem.WPF.DataGridIntersection"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatabaseManagementSystem.WPF"
        mc:Ignorable="d"
        Title="DataGridIntersection" Height="450" Width="800" WindowStartupLocation="CenterScreen">
    <Grid>
        <DataGrid x:Name="TableDataGrid" ItemsSource="{Binding}" CanUserAddRows="False" CanUserReorderColumns="False" IsEnabled="False"/>
    </Grid>
</Window>
