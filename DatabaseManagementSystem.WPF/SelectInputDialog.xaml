<Window x:Class="DatabaseManagementSystem.WPF.SelectInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DatabaseManagementSystem.WPF"
        mc:Ignorable="d"
        Title="SelectInputDialog" Height="200" Width="300">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <ListBox x:Name="ListBoxDatabases" Height="100" Width="199" ItemsSource="{Binding}">
                
            </ListBox>
            <Button Content="Ok" Width="67" Margin="0, 15" Click="OkButton_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
